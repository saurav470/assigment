{
    "installCommand": "npm install && npm run build",
    "buildCommand": "npm start",
    "builds": [
        {
            "src": "build/index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "build/index.js"
        }
    ]
}